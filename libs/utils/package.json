{
  "name": "@satont/grammy-storage-utils",
  "private": true,
  "main": "./dist/cjs/mod.js",
  "module": "./dist/esm/mod.js",
  "exports": {
    ".": {
      "import": "./dist/esm/mod.js",
      "require": "./dist/cjs/mod.js"
    }
  },
  "version": "1.0.3",
  "scripts": {
    "test": "ava",
    "prebuild": "rm -rf dist",
    "build": "deno2node tsconfig.cjs.json && deno2node tsconfig.esm.json && pnpm postbuild",
    "postbuild": "ts-node ../../tools/postBuildFixup.ts --path=dist"
  },
  "keywords": [],
  "dependencies": {
    "grammy": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "ava": "^3.15.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
