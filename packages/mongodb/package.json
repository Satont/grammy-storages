{
  "name": "@satont/grammy-mongodb-storage",
  "version": "1.1.0",
  "description": "MongoDB storage for grammy library.",
  "main": "./dist/cjs/mod.js",
  "module": "./dist/esm/mod.js",
  "exports": {
    ".": {
      "import": "./dist/esm/mod.js",
      "require": "./dist/cjs/mod.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest",
    "prebuild": "rimraf dist",
    "build": "deno2node tsconfig.cjs.json && deno2node tsconfig.esm.json && pnpm postbuild",
    "postbuild": "ts-node ../../tools/postBuildFixup.ts --path=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satont/grammy-storages.git"
  },
  "author": "Satont <satontworldwide@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satont/grammy-storages/issues"
  },
  "homepage": "https://github.com/Satont/grammy-storages/tree/main/packages/mongodb#readme",
  "devDependencies": {
    "@satont/grammy-storage-utils": "workspace:*",
    "@types/node": "^17.0.0",
    "grammy": "^1.5.4",
    "mongodb": "^4.2.2",
    "mongodb-memory-server": "^8.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.7",
    "vitest": "^0.0.131"
  }
}
