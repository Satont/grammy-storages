{
	"name": "@grammyjs/storage-pocketbase",
	"version": "0.2.0",
	"description": "Pocketbase storage adapter for grammY",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"types": "./dist/esm/index.d.ts",
	"files": [
		"README.md",
		"dist",
		"package.json",
		"LICENSE"
	],
	"scripts": {
		"start": "node dist/index.js",
		"build": "tsc -b tsconfig.cjs.json && tsc -b tsconfig.esm.json && pnpm postbuild",
		"postbuild": "tsx ../../tools/postBuildFixup.ts --path=dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/grammyjs/storages.git"
	},
	"author": "ma <mobinaskarimail@protonmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/grammyjs/storages/issues"
	},
	"homepage": "https://github.com/grammyjs/storages#readme",
	"keywords": [
		"grammy",
		"telegram",
		"bot",
		"session",
		"storage",
		"adapter",
		"middleware",
		"pocketbase"
	],
	"devDependencies": {
		"@grammyjs/storage-utils": "workspace:*",
		"grammy": "^1.30.0",
		"pocketbase": "^0.21.5"
	},
	"peerDependencies": {
		"pocketbase": "^0.21.5"
	}
}
