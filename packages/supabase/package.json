{
  "name": "@grammyjs/storage-supabase",
  "version": "2.0.0",
  "description": "Supabase database storage",
  "types": "./dist/esm/mod.d.ts",
  "main": "./dist/cjs/mod.js",
  "module": "./dist/esm/mod.js",
  "exports": {
    ".": {
      "import": "./dist/esm/mod.js",
      "require": "./dist/cjs/mod.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "start": "node dist/mod.js",
    "dev": "nodemon",
    "debug": "node --inspect=0.0.0.0:9229 --nolazy ./dist/mod.js",
    "prebuild": "rimraf dist",
    "build": "deno2node tsconfig.cjs.json && deno2node tsconfig.esm.json && pnpm postbuild",
    "postbuild": "ts-node ../../tools/postBuildFixup.ts --path=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satont/grammy-storages.git"
  },
  "author": "waptik <me@waptik.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satont/grammy-storages/issues"
  },
  "homepage": "https://github.com/Satont/grammy-storages/tree/main/packages/supabase#readme",
  "devDependencies": {
    "@supabase/supabase-js": "^1.24.0",
    "deno2node": "^0.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^1.0.0"
  },
  "keywords": [
    "grammy",
    "grammyjs",
    "bot",
    "telegram",
    "supabase",
    "session",
    "grammy session"
  ]
}