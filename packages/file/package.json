{
  "name": "@satont/grammy-file-storage",
  "version": "1.2.0",
  "description": "File storage for grammY library.",
  "main": "./dist/cjs/mod.js",
  "module": "./dist/esm/mod.js",
  "exports": {
    ".": {
      "import": "./dist/esm/mod.js",
      "require": "./dist/cjs/mod.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest",
    "prebuild": "rimraf dist",
    "build": "deno2node tsconfig.cjs.json && deno2node tsconfig.esm.json && pnpm postbuild",
    "postbuild": "ts-node ../../tools/postBuildFixup.ts --path=dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satont/grammy-storages.git"
  },
  "author": "Satont <satontworldwide@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satont/grammy-storages/issues"
  },
  "homepage": "https://github.com/Satont/grammy-storages/tree/main/packages/file#readme",
  "devDependencies": {
    "@satont/grammy-storage-utils": "workspace:*",
    "@types/node": "^17.0.5",
    "grammy": "^1.5.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.7",
    "vitest": "^0.9.0"
  }
}
